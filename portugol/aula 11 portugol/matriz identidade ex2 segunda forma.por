programa {
  const inteiro L=3, C=3
  funcao inicio() {
    inteiro matriz[L][C]
    
    
      
  para(inteiro k=0;k<L;k++){
      para(inteiro l=0;l<C;l++){
       
        se(k==l){matriz[k][l]=1}
        senao{matriz[k][l]=0}
      escreva(matriz[k][l])
      escreva(" ")}
      escreva("\n")
      }

      
  }

}